{
  "name": "blazor-icons-generator",
  "version": "1.0.0",
  "description": "Icon source downloader and generator for EasyAppDev.Blazor.Icons. Downloads SVG icons from NPM packages and generates C# icon data files for source generation.",
  "scripts": {
    "download-icons": "node download-icons.js",
    "download": "node download-icons.js",
    "generate": "python3 generate-icons.py",
    "setup": "npm run download && npm run generate"
  },
  "keywords": [
    "blazor",
    "icons",
    "svg",
    "lucide",
    "bootstrap",
    "material-design",
    "fontawesome",
    "generator"
  ],
  "dependencies": {
    "lucide-static": "^0.460.0",
    "bootstrap-icons": "^1.11.3",
    "@mdi/svg": "^7.4.47",
    "@fortawesome/fontawesome-free-v5": "npm:@fortawesome/fontawesome-free@5.15.4",
    "@fortawesome/fontawesome-free": "^6.7.2"
  }
}
